  },
  "agents": {
    "defaults": {
      "workspace": "/home/node/.openclaw/workspace",
      "compaction": { "mode": "safeguard" },
      "maxConcurrent": 4,
      "subagents": { "maxConcurrent": 8 },
      "model": {
        "primary": "openai-codex/gpt-5.3-codex",
        "fallbacks": [
          "cliproxy/gemini-2.5-flash",
          "zai/glm-4.7"
        ]
      },
      "models": {
        "openai-codex/gpt-5.3-codex": { "alias": "codex" },
        "cliproxy/claude-sonnet-4-5-20250929": { "alias": "claude" },
        "cliproxy/claude-haiku-4-5-20251001": { "alias": "haiku" },
        "cliproxy/claude-opus-4-6": { "alias": "opus" },
        "cliproxy/gemini-2.5-pro": { "alias": "pro" },
        "cliproxy/gemini-2.5-flash": { "alias": "flash" },
        "cliproxy/gemini-2.5-flash-lite": { "alias": "lite" },
        "zai/glm-4.7": { "alias": "glm" },
        "zai/glm-4.6": { "alias": "glm6" },
        "zai/glm-4.5": { "alias": "glm5" }
      }
    }
  },
  "messages": {
    "ackReactionScope": "group-mentions"
  },
  "commands": {
    "native": "auto",
    "nativeSkills": "auto",
    "restart": true
  },
  "hooks": {
    "internal": {
      "enabled": true,
      "entries": {
        "command-logger": { "enabled": true },
        "session-memory": { "enabled": true }
      }
    }
  },
  "plugins": {
    "entries": {
      "whatsapp": { "enabled": true }
    }
  },
  "channels": {
    "whatsapp": {
      "dmPolicy": "allowlist",
      "allowFrom": ["${WHATSAPP_NUMBER}"],
      "selfChatMode": false,
      "ackReaction": {
